#include <iostream>
using namespace std;
int a[10];
int x= 0;
int main(void)
{
	cout<<"nhap phan tu mang:"<<endl;
	for(int i = 0; i<3;i++)
	{
		 cin>>a[i];
	}
	cout<<"cac so nguyen to trong mang:"<<endl;

	for(int i =0; i<3;i++)
	{
		{
			for (int  j=1; j<=a[i];j++)
			{
				
			 if(a[i]%j==0)
			 {
				 x++;
			 }
			}
			
		if(x==2)
		{

			cout<<a[i];

		}
		}
	}
	
getchar();
return 0;
}
